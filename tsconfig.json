  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["@types/node", "nuxt"],
     "moduleResolution": "node",
    "strict": true,
    "paths": {},
    "baseUrl": "."
  },
  "include": [
    "nuxt.config.ts",
    "server/**/*.ts",
    "composables/**/*.ts",
    "plugins/**/*.ts",
    "components/**/*.vue",
    "pages/**/*.vue"
  ]
}
